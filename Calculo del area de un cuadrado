#include"stdafx.h"
#include"conio.h"
#include <iostream>
using namespace std;

int main() {
	float a;
	float l1;
	float l2;
	cout << "Ingrese el valor del lado 1" << endl;
	cin >> l1;
	cout << "Ingrese el valor del lado 2" << endl;
	cin >> l2;
	a = l1*l2;
	cout << "El valor del area es:" << a << endl;
	return 0;
}
